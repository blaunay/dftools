<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         point="org.eclipse.ui.newWizards">
      <category
            id="net.sf.dftools.architecture.ui.ArchitectureCategory"
            name="Architecture">
      </category>
      <wizard
            canFinishEarly="false"
            category="net.sf.dftools.architecture.ui.ArchitectureCategory"
            class="net.sf.dftools.architecture.ui.ArchitectureProjectWizard"
            hasPages="true"
            id="net.sf.dftools.architecture.ui.ArchitectureProjectWizard"
            name="Architecture Project"
            project="true">
      </wizard>
      <wizard
            canFinishEarly="false"
            category="net.sf.dftools.architecture.ui.ArchitectureCategory"
            class="net.sf.dftools.architecture.ui.NewArchitectureWizard"
            hasPages="true"
            id="net.sf.dftools.architecture.ui.NewArchitectureWizard"
            name="Architecture Description (IP-XACT)"
            project="false">
      </wizard>
   </extension>
   <extension
         point="org.eclipse.ui.editors">
      <editor
            class="net.sf.graphiti.ui.editors.GraphEditor"
            contributorClass="net.sf.graphiti.ui.editors.GraphActionBarContributor"
            default="true"
            id="net.sf.dftools.architecture.ui.editor"
            name="IP-XACT Editor">
      </editor>
   </extension>
   <extension
         point="org.eclipse.ui.views.properties.tabbed.propertyTabs">
      <propertyTabs
            contributorId="net.sf.graphiti.ui.properties.contributor">
         <propertyTab
               category="net.sf.graphiti.ui.properties.category.vertex"
               id="net.sf.dftools.architecture.ui.properties.tab.vertex.parameters"
               label="Component instance parameters">
         </propertyTab>
      </propertyTabs>
   </extension>
   <extension
         point="org.eclipse.ui.views.properties.tabbed.propertySections">
      <propertySections
            contributorId="net.sf.graphiti.ui.properties.contributor">
         <propertySection
               class="net.sf.dftools.architecture.ui.properties.VertexParametersMapSection"
               filter="net.sf.dftools.architecture.ui.properties.ArchitectureVertexFilter"
               id="net.sf.dftools.architecture.ui.properties.section.vertex.parameters"
               tab="net.sf.dftools.architecture.ui.properties.tab.vertex.parameters">
         </propertySection>
      </propertySections>
   </extension>

</plugin>
